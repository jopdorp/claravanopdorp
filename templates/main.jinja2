<html lang="en">
{% load static %}

<head>
  <title>Clara van Opdorp's portfolio - {{ view.current_page.title }}</title>
  <meta charset="utf-8">
  <meta content=" This's a playground to build Brilliant Digital Experiences. Let's play together.." name="description">
  <meta content="Clara van Opdorp's portfolio" property="og:title">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="shortcut icon" href="{%  static '/web/favicon.png' %}">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
  <script src="/static/web/app.js" type="application/javascript"></script>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
  <link rel="stylesheet" href="/static/web/styles.css"/>
</head>

<body>

  {% include "menu.jinja2"%}
  <div class="w-nav-overlay" data-wf-ignore="" id="w-nav-overlay-0"></div>
  <div class="container main-content">
    {% if name == "contact" %}
      {% include "contact.jinja2" %}
{#      we could put an else if here, to check if the view.current_page.path is "works",#}
{#      if so, we render work.jinja2, which will be in charge of rendering its work_items#}
    {% else %}
      {%  if view.current_page.content %}
        <div class="row">
          <div class="col">
            <div>
              {{ view.current_page.content|safe }}
            </div>
          </div>
        </div>
      {% endif %}
      {% for section in view.page_sections %}
        {% include "section.jinja2" %}
      {% endfor %}
    {% endif %}



  </div>


  {% include "footer.jinja2" %}
</body>
</html>
