{% for work in view.works %}
  {% load static %}
  <h4 class="video" id="work-{{ work.title }}" style="display: inline-block" >{{ work.title }}. </h4>
  <h6 id="work-{{ work.content }}" style="display: inline-block">{{ work.content }}</h6>
  <div class="row" id="xwork">
    <div class="col">
      <a href="{{ work.url }}"><video width="100%" src="/media/{{ work.video}}" autoplay muted></video></a>
    </div>
  </div>
{% endfor %}

